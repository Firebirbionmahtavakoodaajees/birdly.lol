{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "cleanUrls": true,
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/index.js" },
      { "source": "/(.*)", "destination": "/" }
    ],
    "headers": [
      {
        "source": "/:path*",
        "headers": [
          { 
            "key": "Cache-Control", 
            "value": "public, max-age=31536000, immutable" 
          }
        ]
      }
    ]
  }

  